<iframe id="<%= id %>" data-src="<%= src %>" data-aspect="<%= aspect %>" style="width:100%" frameborder="0"></iframe><script>(function(){var f=document.getElementById("<%= id %>"),a=parseFloat(f.getAttribute('data-aspect')),w=f.offsetWidth,h=Math.round(w/a),s=f.getAttribute('data-src');f.removeAttribute('id');s+='width='+w+'&height='+h;f.setAttribute('src',s);f.style.height=h+'px';})();</script>